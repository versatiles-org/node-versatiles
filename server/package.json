{
  "name": "@versatiles/server",
  "version": "1.0.0",
  "description": "A server for VersaTiles",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "versatiles-server": "node dist/index.js"
  },
  "scripts": {
    "check": "npm run lint && npm run build && npm run test",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "doc": "npm run -w tsdoc2readme update_readme container/src/index.ts '# API'",
    "lint": "eslint .",
    "start": "tsx src/index.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "type": "module",
  "author": "yetzt <node@yetzt.me>, Michael Kreil <versatiles@michael-kreil.de>",
  "license": "Unlicense",
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "cloudtiles",
    "geo",
    "gis",
    "map",
    "maps",
    "mapserver",
    "maptiles",
    "mvt",
    "opencloudtiles",
    "osm",
    "pbf",
    "tile",
    "tiles",
    "tileserver",
    "vectortiles",
    "versatiles"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "jest": "^29.7.0",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "tar-stream": "^3.1.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@versatiles/container": "^1.0.0",
    "@versatiles/style": "^3.4.1",
    "commander": "^11.1.0"
  }
}