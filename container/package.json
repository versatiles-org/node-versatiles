{
  "name": "@versatiles/container",
  "version": "1.0.0",
  "description": "A client library for reading VersaTiles containers",
  "scripts": {
    "check": "npm run lint && npm run test && npm run build",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "doc": "npm run -w tsdoc2readme update_readme container/src/index.ts '# API'",
    "test": "jest",
    "test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versatiles-org/node-versatiles.git"
  },
  "homepage": "https://github.com/versatiles-org/node-versatiles/blob/main/container/README.md",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "yetzt <node@yetzt.me>, Michael Kreil <versatiles@michael-kreil.de>",
  "license": "Unlicense",
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "cloudtiles",
    "geo",
    "gis",
    "map",
    "maps",
    "mapserver",
    "maptiles",
    "mvt",
    "opencloudtiles",
    "osm",
    "pbf",
    "tile",
    "tiles",
    "tileserver",
    "vectortiles",
    "versatiles"
  ]
}